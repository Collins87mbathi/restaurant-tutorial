{
    "name": "restaurant-tutorial",
    "version": "1.0.0",
    "description": "Tutorial for restaurant site using MERN stack",
    "main": "server.js",
    "engines": {
        "node": "12.14.0",
        "npm": "6.13.4"
    },
    "scripts": {
        "start": "node server.js",
